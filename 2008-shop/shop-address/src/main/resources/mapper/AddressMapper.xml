<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qf.mapper.AddressMapper">


    <insert id="addAddress" statementType="CALLABLE">
        {
          call save_address
          (
              #{username},
              #{phone},
              #{address},
              #{uid},
              #{def}
          )
        }
    </insert>

</mapper>